    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rbayuokt.coronatracker",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.rbayuokt.coronatracker",
      "versionCode": 3,
      "permissions": [
        "CAMERA",
        "READ_CONTACTS",
        "RECORD_AUDIO",
        "READ_PHONE_STATE"
      ]
    }
  }
}
