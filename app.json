{
  "expo": {
    "name": "Corona Tracker",
    "description" : "Aplikasi tracker covid-19",
    "slug": "CoronaTracker",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bacteria.png",
